{
  "name": "masterlist-nodejs",
  "version": "1.0.0",
  "description": "A comprehensive project management and analytics platform built with Next.js",
  "private": true,
  "author": "Masterlist Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/masterlist.git"
  },
  "scripts": {
    "dev": "next dev",
    "dev:socket": "node server.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "format": "prettier --write .",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "import-original-data": "node scripts/import-original-data.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:backup": "node scripts/export-db-backup.js",
    "db:reset": "prisma migrate reset --force",
    "setup": "npm install && npm run db:generate && npm run db:push && npm run db:seed",
    "setup:fresh": "npm install && npm run db:generate && npm run db:reset && npm run db:seed"
  },
